<div class="authform">
  <h3>编辑用户</h3>
  <%= form_for(@user, :url => update_for_omniauth_admin_user_path(@user)) do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, :autofocus => true, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>
    </div>
    <%= f.submit '提交', :class => 'button right' %>
  <% end %>
</div>